<Defs>
    <ThingDef ParentName="HatMakeableBase">
        <defName>SFE_Apparel_NomadHeadWrap</defName>
        <label>nomad head wrap</label>
        <description>A headwrap helps to survive the heat, as well as the coolness of the night.</description>
        <graphicData>
            <texPath>Apparel/Headgear/NomadHeadWarp/Nomad_Head_Wrap</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <maskPath>Apparel/Headgear/NomadHeadWarp/Nomad_Head_Wrap_m</maskPath>
            <shaderType>CutoutComplex</shaderType>
            <color>(94, 96, 103)</color>

        </graphicData>
        <uiIconScale>1.25</uiIconScale>
        <costStuffCount>50</costStuffCount>
        <stuffCategories>
            <li>Fabric</li>
        </stuffCategories>
        <statBases>
            <MaxHitPoints>80</MaxHitPoints>
            <WorkToMake>60</WorkToMake>
            <Mass>0.2</Mass>
            <StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
            <StuffEffectMultiplierInsulation_Cold>0.25</StuffEffectMultiplierInsulation_Cold>
            <StuffEffectMultiplierInsulation_Heat>0.4</StuffEffectMultiplierInsulation_Heat>
            <EquipDelay>2</EquipDelay>
        </statBases>
        <generateCommonality>0.5</generateCommonality>
        <apparel>
            <countsAsClothingForNudity>true</countsAsClothingForNudity>
            <developmentalStageFilter>Child, Adult</developmentalStageFilter>
            <bodyPartGroups>
                <li>UpperHead</li>
            </bodyPartGroups>
            <wornGraphicPath>Apparel/Headgear/NomadHeadWarp/Nomad_Head_Wrap</wornGraphicPath>
            <layers>
                <li>Overhead</li>
            </layers>
            <tags>
                <li>MedievalMilitary</li>
            </tags>
            <defaultOutfitTags>
                <li>Worker</li>
            </defaultOutfitTags>
        </apparel>
        <colorGenerator Class="ColorGenerator_Options">
            <options>
                <li>
                    <weight>10</weight>
                    <only>(0.33,0.33,0.33,1)</only>
                </li>
                <li>
                    <weight>10</weight>
                    <only>(0.65,0.65,0.65,1)</only>
                </li>
                <li>
                    <weight>6</weight>
                    <min>(0.3,0.3,0.3,1)</min>
                    <max>(0.5,0.5,0.5,1)</max>
                </li>
                <li>
                    <weight>10</weight>
                    <only>(0.9,0.9,0.9,1)</only>
                </li>
            </options>
        </colorGenerator>
    </ThingDef>
